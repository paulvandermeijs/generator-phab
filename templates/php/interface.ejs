<% if (locals.comment) { -%>
    <%- include('comment', { ...comment, locals: comment }) %>
<% } -%>
interface <%= name %> <% if (locals.extends) { %><%- include('interface/extends', { name: locals.extends }) %><% } %> <% if (locals.implements) { %><%- include('interface/implements', locals) %><% } %>
{
    <% (locals.consts || []).forEach(function (con) { -%>
        <%- include('interface/const', { ...con, locals: con }); %>
    <% }) -%>
    
    <% (locals.functions || []).forEach(function (fun) { -%>
        <%- include('interface/function', { ...fun, locals: fun }); %>
    <% }) -%>
}
